<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:style>
.group, .group > a {
	padding: 0;  
}
	</ui:style>
	<div class="actionsMenu">
	
		<span class="btn-group {style.group}">
			<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
			    <span data-html="Places">Places</span>				 
				<span class="caret"></span>
			</a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
				<li>
					<a tabindex="-1" href="#" ui:field="actionAddPlaceAnchor" data-html="Add Place">Add Place</a>
				</li>
				<li>
					<a tabindex="-1" href="#" ui:field="actionSearchPlaceAnchor" data-html="Search Places">Search Places</a>
				</li>
				<li>
					<a tabindex="-1" href="#" data-html="My Places">My Places</a>
				</li>
			</ul>
		</span>
		
		<span class="btn-group {style.group}">
			<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
				<span  data-html="Maps">Maps</span>
				<span class="caret"></span>
			</a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
				<li>
					<a tabindex="-1" href="#" ui:field="actionAddMapView" data-html="Add Map">Add Map</a>
				</li>
				<li>
					<a tabindex="-1" href="#" ui:field="actionSearchMapView" data-html="Search Map">Search Map</a>
				</li>
				<li>
					<a tabindex="-1" href="#" data-html="My Maps">My Maps</a>
				</li>
			</ul>
		</span>

		<span class="btn-group {style.group}">
			<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
			    <span data-html="Tools">Tools</span>
				<span class="caret"></span>
			</a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
				<li>
					<a tabindex="-1" href="#" ui:field="actionSearchAddressAnchor" data-html="Search Address">Search Address</a>
				</li>
				<li>
					<a tabindex="-1" href="#" ui:field="actionMeasureDistanceAnchor" data-html="Measure Distance">Measure Distance</a>
				</li>
				<li>
					<a tabindex="-1" href="#" ui:field="toolDirectionsAnchor" data-html="Directions">Directions</a>
				</li>
				<li>
					<a tabindex="-1" href="#" ui:field="toolElevationAnchor" data-html="Elevations">Elevations</a>
				</li>
			</ul>
		</span>

		<span class="btn-group {style.group}">
			<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
				<span data-html="Map Layers">Map Layers</span>
				<span class="caret"></span>
			</a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="checkbox" ui:field="layerTrafficCheck">
							</input>
							<span data-html="Traffic">Traffic</span>
							
						</label>
					</a>
				</li>
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="checkbox" ui:field="layerTransitCheck">
							</input>
							<span data-html="Transit">Transit</span>
							
						</label>
					</a>
				</li>
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="checkbox" ui:field="layerBicyclingCheck">
							</input>
							<span data-html="Bicycling">Bicycling</span>
							
						</label>
					</a>
				</li>
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="checkbox" ui:field="layerCloudCheck">
							</input>
							<span data-html="Weather and Clouds">Weather and Clouds</span>
							
						</label>
					</a>
				</li>
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="checkbox" ui:field="layerPanoramioCheck">
							</input>
							<span data-html="Panoramio">Panoramio</span>
							
						</label>
					</a>
				</li>
			</ul>
		</span>


		<span class="btn-group {style.group}">
			<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
				Map Type
				<span class="caret"></span>
			</a>
			<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="radio" name="groupMapType" checked="true" ui:field="mapTypeRoadmapAnchor">
							</input>
							Roadmap
						</label>
					</a>
				</li>

				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="radio" name="groupMapType" ui:field="mapTypeSatelliteAnchor">
							</input>
							Satellite
						</label>
					</a>
				</li>

				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="radio" name="groupMapType" ui:field="mapTypeHybridAnchor">
							</input>
							Hybrid
						</label>
					</a>
				</li>

				<li>
					<a tabindex="-1" href="#">
						<label class="checkbox">
							<input type="radio" name="groupMapType" ui:field="mapTypeTerrainAnchor">
							</input>
							Terrain
						</label>
					</a>
				</li>

			</ul>
		</span>
		
		<div ui:field="actionOptionPanelEl">
		</div>

	</div>
</ui:UiBinder>