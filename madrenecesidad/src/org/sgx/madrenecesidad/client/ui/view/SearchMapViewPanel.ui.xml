<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:style>
.button{padding-left: 4px; margin-top: 2px; }
.results{display: none; }
	</ui:style>
	<div>
	    <div class="form form-inline">
	        <label>Keywords</label>
	        <input type="text" ui:field="keywordsEl"></input>
	    </div>
	    <span class="btn btn-primary {style.button}" ui:field="searchButton">Search Map!</span>
	    <div class="{style.results}" ui:field="resultsEl"></div>
	</div>
</ui:UiBinder>